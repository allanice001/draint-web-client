  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "update-snapshots": "react-scripts test --updateSnapshot",
    "lint": "eslint src/**/*.js{,x}",
    "eject": "react-scripts eject",
    "ec2-deploy": "ssh ubuntu@ec2-3-9-149-162.eu-west-2.compute.amazonaws.com 'cd /srv/draint-web-client/ && sudo git stash && sudo git pull && sudo yarn install && sudo yarn build'",
    "ec2-deploy-staging": "ssh ubuntu@staging.draint.art 'cd /srv/www && git stash && git pull && cp src/settings-staging.json src/settings.json && sudo yarn install && NODE_ENV=staging sudo yarn build'",
    "zip-assets": "zip -rq build.zip build/",
    "ec2-copy-assets-staging": "scp -i ./ci/keys/staging -o \"StrictHostKeyChecking no\" -o \"UserKnownHostsFile /dev/null\" -C -r ./build.zip ubuntu@staging.draint.art:/srv/www/build.zip",
    "ec2-copy-assets-prod": "scp -i ./ci/keys/prod -o \"StrictHostKeyChecking no\" -o \"UserKnownHostsFile /dev/null\" -C -r ./build.zip ubuntu@ec2-3-9-149-162.eu-west-2.compute.amazonaws.com:/srv/draint-web-client/build.zip",
    "ec2-apply-assets-staging": "ssh -i ./ci/keys/staging -o \"StrictHostKeyChecking no\" -o \"UserKnownHostsFile /dev/null\" ubuntu@staging.draint.art 'rm -rf /srv/www/build && unzip -q /srv/www/build.zip -d /srv/www'",
    "ec2-apply-assets-prod": "ssh -i ./ci/keys/prod -o \"StrictHostKeyChecking no\" -o \"UserKnownHostsFile /dev/null\" ubuntu@ec2-3-9-149-162.eu-west-2.compute.amazonaws.com 'rm -rf /srv/draint-web-client/build && unzip -q /srv/draint-web-client/build.zip -d /srv/draint-web-client'",
    "predeploy": "REACT_APP_STAGE=staging yarn build",
    "deploy": "aws s3 sync build/ s3://client.draint.art",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E24F3JWPL7HGQS --paths '/*'",
    "predeploy:prod": "REACT_APP_STAGE=production yarn build",
    "deploy:prod": "aws s3 sync build/ s3://draint.art",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id E24F3JWPL7HGQS --paths '/*'",
    "staging:redesign_mode": "ssh ubuntu@staging.draint.art 'cd /srv/www && git stash && git checkout redesign && git pull origin redesign && cp src/settings-staging.json src/settings.json && sudo yarn install && NODE_ENV=staging sudo yarn build'",
    "staging:develop_mode": "ssh ubuntu@staging.draint.art 'cd /srv/www && git stash && git checkout develop && git pull origin develop && cp src/settings-staging.json src/settings.json && sudo yarn install && NODE_ENV=staging sudo yarn build'",
    "stylelint": "stylelint src -s \"scss\"",
    "stylelint-fix": "stylelint src -s \"scss\" --fix"
  },
